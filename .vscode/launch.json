{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
      {
        "name": "Attach to Next.js Server",
        "type": "node",
        "request": "attach",
        "address": "localhost",
        "port": 9229,
        "restart": true,
        "protocol": "inspector",
        "skipFiles": ["<node_internals>/**", "node_modules/**"]
      }
    ]
  }
  